{
  "name": "neuros",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:local": "next dev -H 0.0.0.0 -p 3001",
    "dev:80": "sudo next dev -H 0.0.0.0 -p 80",
    "proxy:80": "sudo node proxy-80.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config=playwright-background.config.js",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:report": "node scripts/generate-test-manifest.js",
    "browser:clean": "./scripts/browser-helper.sh clean",
    "browser:setup": "./scripts/browser-helper.sh setup",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types --local > types/supabase.ts",
    "prepare": "husky",
    "ai:start": "./start-ai-company.sh",
    "ai:quality": "node scripts/ai-quality-monitor.js",
    "ai:quality:fix": "node scripts/ai-quality-monitor.js --fix",
    "ai:optimize": "python3 scripts/self-learning-optimizer.py",
    "ai:deploy": "tsx scripts/ai-deployment-manager.ts",
    "ai:deploy:check": "tsx scripts/ai-deployment-manager.ts --check"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
